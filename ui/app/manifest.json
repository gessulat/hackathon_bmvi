{
    "_version": "1.1.0",
    "sap.app": {
        "_version": "1.1.0",
        "id": "bmvi.ui.app",
        "type": "application",
        "i18n": "i18n/messagebundle.properties",
        "title": "{{APP_TITLE}}",
        "description": "{{APP_DESCRIPTION}}",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "dataSources": {
            "default": {
                "uri": "../data/data.json",
                "type": "JSON"
            }
        }
    },
    "sap.ui": {
        "_version": "1.1.0",
        "technology": "UI5",
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        },
        "supportedThemes": [
            "sap_bluecrystal",
            "sap_hcb"
        ]
    },
    "sap.ui5": {
        "_version": "1.1.0",
        "resourceRoots": {
            "bmvi.ui.app": "."
        },
        "dependencies": {
            "minUI5Version": "1.32.5",
            "libs": {
                "sap.m": {
                    "minVersion": "1.32.5"
                },
                "bmvi.ui.app.lib": {
                    "minVersion": "1.0.0"
                }
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/styles.css"
                }
            ]
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "bmvi.ui.app.i18n.messagebundle"
                }
            },
            "": {
                "type": "sap.ui.model.json.JSONModel",
                "dataSource": "default"
            }
        },
        "rootView": "bmvi.ui.app.view.App",
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "viewPath": "bmvi.ui.app.view",
                "controlId": "app",
                "controlAggregation": "pages",
                "transition": "slide",
                "bypassed": {
                    "target": "notFound"
                }
            },
            "routes": [
                {
                    "pattern": "",
                    "name": "home",
                    "target": "home"
                }
            ],
            "targets": {
                "home": {
                    "viewName": "Home",
                    "viewLevel": 1
                },
                "notFound": {
                    "viewName": "NotFound",
                    "transition": "show"
                }
            }
        }
    }
}
